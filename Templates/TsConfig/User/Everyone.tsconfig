
options {
   # https://docs.typo3.org/m/typo3/reference-tsconfig/main/en-us/UserTsconfig/Options.html#clearcache-all
   clearCache {
      pages = 1
      system = 1
      all = 1
   }

   # https://docs.typo3.org/m/typo3/reference-tsconfig/main/en-us/UserTsconfig/Options.html#file-list
   file_list {
      thumbnail {
         width = 300
         height = 300
      }
   }
}
